<table class="userTable">
    <tr class="userTableHeader">
      <th>First Name</th>
      <th>Last Name</th>
      <th>Position</th>
      <th>Email</th>
      <% let renderInsertPage = `/users/insert/${userInfo.username}` %>
      <th>Action <a class="activeBtn" href=<%= renderInsertPage %>>+</a></th>
    </tr>
    <% faculty.forEach(element => { %>
        <tr class="userTableRow">
          <td><%= element.firstName %></td>
          <td><%= element.lastName %></td>
          <td><%= element.jobPosition %></td>
          <td><%= element.email %></td>
          <% if (element.hasAccount === true) { %>
              <td class="userTable-action"><a class="disabledBtn" href="#">Create User Account</a></td>
          <% } else{ %>
            <% let createAccRoute = `/users/create/${element._id}/${userInfo.username}` %>
              <td class="userTable-action"><a class="activeBtn" href="<%= createAccRoute %>">Create User Account</a></td>
          <% } %>
        </tr>
    <% }) %>
</table>