<%- include('./partials/sidebar.ejs') %> -->
<div class="wrapper">
    <div class="left">
        <img src="/views/img/user_img.png" alt="user" width="100"/>
        <h4><%= userInfo.firstName + " " + userInfo.lastName%></h4>
        <h5><%= userInfo.companyName%></h5>
        <p><%= userInfo.jobPosition %></p>
    </div>
    <div class="right">
        <div class="info">
            <h3>Information</h3>
            <div class="info_data">
                 <div class="data">
                    <h4>Email</h4>
                    <p><%= userInfo.email %></p>
                 </div>
                 <div class="data">
                    <h4>Phone</h4>
                    <p><%= userInfo.phoneNumber %></p>
              </div>
            </div>
        </div>
      
        <div class="social_media">
            <ul>
              <li><a href="#"><i class="fab fa-facebook-f"></i></a></li>
              <li><a href="#"><i class="fab fa-twitter"></i></a></li>
              <li><a href="#"><i class="fab fa-instagram"></i></a></li>
          </ul>
      </div>
    </div>
</div>